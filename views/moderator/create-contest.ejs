  <body style=" background: linear-gradient(to bottom,#163a37, #051613); ">
    <%- include('../includes/moderatorNav.ejs') %>  
    <main id="main" class="px-4 text-white">

    

          <hr class="mb-2 bg-dark" ><h3 class="mb-3 text-center text-white p-3" style="background-color: darkblue;"><i>Create Contest </i></h3><hr>
         
            <form action="/moderator/create-contest" method="POST">
              
              <div class="form-row mb-4">
                <div class="col-md-6 mb-3">
                  <label for="contestName">Contest Name</label>
                  <input type="text" class="form-control" id="contestName" name="contestName" placeholder="Contest Name" required>
                  
                </div>
                <div class="col-md-6 mb-3">
                  <label for="contestFee">Contest Fee</label>
                  <input type="number" class="form-control" id="contestFee" name="contestFee" placeholder="Contest Fee" >
                  
                </div>
                
              </div>
              <div class="form-row mb-4">
                <div class="col-md-6 mb-4">
                  <label for="contestDate">Contest Date</label>
                  <input type="date" class="form-control" id="contestDate"name="contestDate" required>
                
                  
                </div>
                <div class="col-md-6 mb-3">
                  <label for="contestTime">Contest Time</label>
                  <input type="time" class="form-control" id="contestTime" name="contestTime" id="contestTime" required>
                  
                </div>
               
               
              </div>


              <div class="form-row mb-4">
                <div class="col-md-4 mb-4">
                  <label for="topic">Contest Type</label>
                  <select id="topic" class="form-control" name="contestId">
                    <option selected>Choose...</option>
                    <% contestTopics.forEach( topic => { %>
                    <option value="<%=topic.id%>"><%=topic.contestTypeName%></option>
                    <%})%>
                  </select>
                </div>
                <div class="col-md-4 mb-4">
                  <label for="topic">Contest Topic / Chapter</label>
                  <select id="topic" class="form-control" name="topic">
                    <option selected value="">Choose...</option>
                    <option value="Chapter 1">Chapter 1</option>
                    <option value="Chapter 2">Chapter 2</option>
                    <option value="Chapter 3">Chapter 3</option>
                    <option value="Chapter 4">Chapter 4</option>
                    <option value="Chapter 5">Chapter 5</option>
                    <option value="Chapter 6">Chapter 6</option>
                    <option value="Chapter 7">Chapter 7</option>
                    <option value="Chapter 8">Chapter 8</option>
                    <option value="Chapter 9">Chapter 9</option>
                    <option value="Chapter 10">Chapter 10</option>
                    <option value="Chapter 11">Chapter 11</option>
                    <option value="Chapter 12">Chapter 12</option>
                    <option value="Chapter 13">Chapter 13</option>
                    <option value="Chapter 14">Chapter 14</option>
                    <option value="Chapter 14">Chapter 15</option>
                    <option value="Model Test">Model Test</option>
                   
                    
                  </select>
                </div>
                <div class="col-md-4 mb-3">
                  <label for="contestType">Contest Payment</label>
                  <select id="contestType" class="form-control" name="contestType">
                    <option selected>Choose...</option>
                   
                    <option value="1">Paid</option>
                    <option value="0">Free</option>
                  </select>
                </div>
               
               
              </div>
              <input type="hidden" name="_csrf" value="<%= csrfToken %>">
              <div class="d-flex justify-content-center mb-3">
                <button class="btn btn-lg btn-primary "  type="submit">Create </button>
              </div>
             
            </form>
            
            <script>
            
            </script>
           
           
           
        


                


              
              
           
  


       
    </main>

