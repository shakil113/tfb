  <body style=" background: linear-gradient(to bottom,#163a37, #051613); ">
    <%- include('../includes/adminNav.ejs') %>  
    <main id="main" class="px-4 text-white">

    

          <hr class="mb-2 bg-dark" ><h3 class="mb-3 text-center text-white p-3" style="background-color: darkblue;"><i>Add Contest Payment</i></h3><hr>
         
            <form action="/admin/add-contest-payment" method="POST">
              
              <div class="form-row mb-4">
                    <div class="col-md-2 mb-3">
                        <label for="Position">Position</label>
                        <input type="number" class="form-control" id="Position" name="position" placeholder="Position" required>
                        
                    </div>

                    <div class="col-md-2 mb-3">
                        <label for="from10to1k">From 10 to 1k</label>
                        <input type="number" class="form-control" id="from10to1k" name="from10to1k" placeholder="From 10 to 1k" required>
                        
                    </div> 

                    <div class="col-md-2 mb-3">
                        <label for="from1kto4k">From 1k to 4k</label>
                        <input type="number" class="form-control" id="from1kto4k" name="from1kto4k" placeholder="From 1k to 4k" required>
                        
                    </div>

                    <div class="col-md-2 mb-3">
                        <label for="from5kto9k">From 5k to 9k</label>
                        <input type="number" class="form-control" id="from5kto9k" name="from5kto9k" placeholder="From 5k to 9k" required>
                        
                    </div>

                    <div class="col-md-2 mb-3">
                        <label for="from10kto20k">From 10k to 20k</label>
                        <input type="number" class="form-control" id="from10kto20k" name="from10kto20k" placeholder="From 5k to 9k" required>
                        
                    </div>

                    <div class="col-md-2 mb-3">
                        <label for="from20k">From 20k </label>
                        <input type="number" class="form-control" id="from20k" name="from20k" placeholder="From 20k to ..." required>
                        
                    </div>
                
              </div>

              


                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                <div class="d-flex justify-content-center mb-3">
                    <button class="btn btn-lg btn-primary "  type="submit">Add</button>
                </div>
             
            </form>
            
            
           
           
           
        


                


              
              
           
  


       
    </main>

